    <skos:closeMatch rdf:resource="http://rdf.freebase.com/ns/m.05pd6"/>
    <skos:broader rdf:resource="http://data.europeana.eu/concept/music/genre/Classical_music"/>
    <skos:definition xml:lang="en">Of, relating to, or composed for an orchestra.  Suggestive of an orchestra or its musical qualities.</skos:definition>
    <skos:exactMatch xml:lang="en">http://dbpedia.org/resource/Orchestra</skos:exactMatch>
    <skos:inScheme rdf:resource="http://data.europeana.eu/concept/music/genre/Music_Genre_Scheme"/>
